<resources>

    <style name="AppTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="android:statusBarColor">#fff</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowBackground">#ffff</item>

        <item name="android:navigationBarColor">#448AFF</item>

        <item name="actionBarStyle">@style/MyActionBar</item>

    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="titleTextStyle">@style/MyTitleTextStyle</item>
    </style>

    <style name="MyTitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">#000</item>
    </style>

    <style name="NasaSolidButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@drawable/bg_button_solid</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="NasaOutlinedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@drawable/bg_button_outlined</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/dark_app_color</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:gravity">center</item>
    </style>

